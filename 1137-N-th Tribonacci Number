class Solution {
public:
    int tribonacci(int n) {
        for(int i=0;i<n;i++)
        {
         number(i);
        }
        return number(n);
    }
    
    int number(int n)
    {
        if(n<=1)
            return n;
        if(n==2)
            return 1;
        else
            return number(n-1)+number(n-2)+number(n-3);
    }
    
};
